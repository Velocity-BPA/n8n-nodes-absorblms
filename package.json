{
  "name": "n8n-nodes-absorblms",
  "version": "1.0.0",
  "description": "n8n community node for AbsorbLMS integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Velocity BPA",
    "email": "support@velocitybpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/velocitybpa/n8n-nodes-absorblms.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AbsorbLMSApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AbsorbLMS/AbsorbLMS.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0"
  },
  "dependencies": {
    "n8n-core": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
